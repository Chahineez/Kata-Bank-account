		user1.deposit(10);
		System.out.println("votre solde: "+user1.getBalance());

		
		// Retrait  de 200
		user1.withdrawal(200);
		// retrait de 20
		user1.withdrawal(20);
		// depot de 100
		user1.deposit(100);
		// retrait de 20
		user1.withdrawal(20);
		// depot de 10
		user1.deposit(10);
	
		// statement
		user1.print();

	}

